<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-800 p-4 text-white">
        <div class="cards w-full flex flex-wrap gap-4">
            <% user.posts.forEach(function(elem){ %>
            <div class="w-32 card">
                <div class=" w-full h-40 bg-zinc-300 rounded-lg overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="mt-1 text-sm"><%= elem.title %></h5>
            <% }) %>
            </div>
            
        </div>
    </div>
<% include ./partials/footer.ejs %>